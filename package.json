  "devDependencies": {
    "babel-preset-nano-react-app": "^0.1.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "gh-pages": "^3.2.3",
    "parcel": "2.2.1",
    "sass": "^1.49.7",
    "vite": "^2.6.7"
